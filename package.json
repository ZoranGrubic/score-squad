{
  "name": "score-squad",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:seed": "supabase db seed",
    "db:link": "supabase link --project-ref $(grep EXPO_PUBLIC_SUPABASE_URL .env | cut -d '/' -f3 | cut -d '.' -f1)",
    "db:generate-types": "supabase gen types typescript --local > lib/database.types.ts",
    "db:new-migration": "supabase migration new",
    "functions:deploy": "supabase functions deploy",
    "functions:deploy-sync": "supabase functions deploy sync-competitions",
    "functions:deploy-cron": "supabase functions deploy cron-sync-competitions",
    "functions:deploy-cron-all": "supabase functions deploy cron-sync-all-data",
    "functions:invoke-sync": "curl -X POST \"$(grep EXPO_PUBLIC_SUPABASE_URL .env | cut -d '=' -f2)/functions/v1/sync-competitions\" -H \"Authorization: Bearer $(grep EXPO_PUBLIC_SUPABASE_ANON_KEY .env | cut -d '=' -f2)\" -H \"Content-Type: application/json\" -H \"X-Auth-Token: 419ad0daf3734b2fae1eaf262e8edd0f\"",
    "functions:invoke-sync-all": "curl -X POST \"$(grep EXPO_PUBLIC_SUPABASE_URL .env | cut -d '=' -f2)/functions/v1/cron-sync-all-data\" -H \"Authorization: Bearer $(grep EXPO_PUBLIC_SUPABASE_ANON_KEY .env | cut -d '=' -f2)\" -H \"Content-Type: application/json\" -H \"X-Auth-Token: 419ad0daf3734b2fae1eaf262e8edd0f\"",
    "functions:logs": "supabase functions logs",
    "cron:setup": "supabase db push && echo 'Cron job setup completed! The sync will run daily at 6:00 AM.'",
    "cron:status": "supabase db functions invoke get_cron_jobs"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.57.4",
    "expo": "~54.0.6",
    "expo-constants": "~18.0.8",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-navigation-bar": "^5.0.8",
    "expo-router": "~6.0.3",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
